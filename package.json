    "datatables.net-bs": "^1.10.15",
    "datatables.net-responsive": "^2.1.1",
    "font-awesome": "^4.7.0",
    "interactjs": "^1.2.8",
    "jasny-bootstrap": "^3.1.3",
    "jquery": "^3.2.1",
    "jquery-slimscroll": "^1.3.8",
    "js-cookie": "^2.1.4",
    "metismenu": "^2.7.0",
    "moment-timezone": "^0.5.13",
