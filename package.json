{
  "name": "mxtracking",
  "version": "0.1.0",
  "description": "BoutiqueAir Maintenance Tracking System",
  "keywords": [
    "mxtracking",
    "django",
    "webpack"
  ],
  "scripts": {
    "watch": "webpack-dev-server --config webpack/webpack.dev.config.js --port 8080",
    "build": "npm install && NODE_ENV=production ./node_modules/.bin/webpack --config webpack/webpack.prod.config.js",
    "test:backend": "python manage.py test",
    "test:backend:coverage": "coverage run --source='.' manage.py test && coverage report",
    "test:frontend": "BABEL_ENV=test karma start frontend/test/unit/karma.conf.js --single-run"
  },
